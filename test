if Fov == 4 then FH5() end
if Fov == 5 then FH10() end
if Fov == nil then hide() end
HOMEDM=-1
end


function FH2() 
local old = gg.getRanges();
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber('360', gg.TYPE_FLOAT)
gg.searchAddress('40',FF)
gg.getResults(5)
gg.editAll(300, gg.TYPE_FLOAT)
gg.clearResults()
gg.toast('Fov Changed')
gg.setRanges(old)
end


