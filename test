function nr()
gg.setRanges(gg.REGION_CODE_APP)
    gg.searchNumber("DB0F4940r", gg.TYPE_FLOAT)
    gg.getResults(1)
    gg.addListItems({
      [1] = {
        address = 3307425376,
        flags = 16,
        value = 3.1415927410125732
      }
    })
    gg.editAll("00000000r", gg.TYPE_FLOAT)
    gg.clearResults()
    gg.toast("No Recoil Activated")
end

