HOMHOMHOME=1
function HOME()
AB = gg.choice({'32 Bit ',' 64 Bit',' Crash Game after Match Ends  [32 BIT]','Kill Script'},nil,'Pick your Device Architecture')
if AB == 1 then threetwo() end
if AB == 2 then sixfor() end
if AB == 3 then crash() end
if AB == 4 then os.exit() end
if AB == nil then hide() end
HOMEDM=-1
end

function crash() 
local old = gg.getRanges();
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber('0.1', gg.TYPE_FLOAT, false)
gg.getResults(1000)
gg.refineNumber('0.1')
gg.getResults(1000)
gg.refineNumber('0.1')
gg.getResults(1000)
gg.refineNumber('0.1')
gg.getResults(200)
gg.editAll(99, gg.TYPE_FLOAT)
gg.clearResults()
gg.toast('Game Will Crash as Match Ends')
gg.setRanges(old)
end

function threetwo()
three = gg.choice({'Legit Hacks ',' Extreme Hacks ','Back'},nil,'x32(x86) (armeabi-v7a)')
if three == 1 then LH() end
if three == 2 then EX() end
if three == 3 then HOME() end
if three == nil then hide() end
HOMEDM=-1
end


 function LH()
legit = gg.choice ({'Radar ',' No Recoil/Spread','No Smoke/Flash','X2 FireRate','Aim Assist Boost','FOV','No AimPunch','Back','Hide Script'},nil,'Legit Hacks')
if legit == 1 then rad() end
if legit == 2 then nr() end
if legit == 3 then nsf() end
if legit == 4 then firer() end
if legit == 5 then aab() end
if legit == 6 then FOV() end
if legit == 7 then nap() end
if legit == 8 then threetwo() end
if legit == nil then hide() end
HOMEDM=-1
end

function EX()
extreme = gg.choice({'Team Radar(UNTESTED)','UnderGround','Fly','X3.5 FireRate','Giant Body','Larger HitBox','Shoot Through Walls','Magic Bullets','Noclip','WallClimb','Rain','Back'},nil,'Extreme Hacks')
if extreme == 1 then ug() end
if extreme == 2 then fly() end
if extreme == 3 then firerr() end
if extreme == 4 then gb() end
if extreme == 5 then lbh() end
if extreme == 6 then stw() end
if extreme == 7 then mb() end
if extreme == 8 then nc() end
if extreme == 9 then wc() end
if extreme == 10 then raim() end
if extreme == 11 then threetwo() end
if extreme == nil then hide() end
end

function rain()
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber("00000000r;1EFF2FE1r;F04F2DE9r;1CB08DE2r;068B2DEDr;08708BE2r", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("0", gg.TYPE_DWORD)
gg.getResults(1)
gg.addListItems({ 
	[1] = { 
		['address'] = 0xc2fb26e0,
		['flags'] = 4,
		['value'] = 0,
	},
})
gg.editAll("00009841r", gg.TYPE_DWORD)
gg.clearResults()


gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber("0700A0E10010A0E3r;010050E33800001Ar;104A08EE144097E5r", gg.TYPE_QWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("010050E33800001Ar", gg.TYPE_QWORD)
gg.getResults(1)
gg.addListItems({ 
	[1] = { 
		['address'] = 0xc2fb239c,
		['flags'] = 32, 
		['value'] = 1873497689317965825,
	},
})
gg.editAll("000050E33800001Ar", gg.TYPE_QWORD)
gg.clearResults()
gg.toast('Rain Activated')
end

function sixfor()
six = gg.choice({'Legit Hacks','Extreme Hacks','Back'},nil,'x64 Bit(arm64)')
if six == 1 then LH1() end
if six == 2 then EX1() end
if six == 3 then HOME() end
if six == nil then hide() end
end

function LH1()
